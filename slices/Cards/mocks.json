      {
        "__TYPE__": "GroupItemContent",
        "value": [
          [
            "title",
            {
              "__TYPE__": "FieldContent",
              "type": "Text",
              "value": "Transparency"
            }
          ],
          [
            "description",
            {
              "__TYPE__": "StructuredTextContent",
              "value": [
                {
                  "type": "paragraph",
                  "content": {
                    "text": "We’re offering an alternative to the lack of transparency in financial services, especially in the cross border, p2p and payments space. To that end, we’ll create world class products to solve your financial needs, price them fairly and tell you EXACTLY what you’re paying for.",
                    "spans": []
                  },
                  "direction": "ltr"
                }
              ]
            }
          ],
          [
            "hilight_description",
            {
              "__TYPE__": "StructuredTextContent",
              "value": [
                {
                  "type": "paragraph",
                  "content": {
                    "text": "We believe that we shouldn’t have to resort to tricks to earn an income by serving you, all we need to do is wow you with our products and customer service; so we focus our energy on building delightful experiences for you.",
                    "spans": []
                  },
                  "direction": "ltr"
                }
              ]
            }
          ],
          [
            "icon",
            {
              "url": "https://images.unsplash.com/photo-1528821128474-27f963b062bf?crop=entropy&cs=tinysrgb&fm=jpg&ixid=MnwzMzc0NjN8MHwxfHNlYXJjaHwxM3x8ZnJ1aXR8ZW58MHx8fHwxNjczMzg3NzA1&ixlib=rb-4.0.3&q=80&rect=963%2C614%2C3172%2C2115&w=5170&h=3447",
